{
  "name": "store-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "create-db": "db-migrate --config database_create.json db:create store && db-migrate up -e dev",
    "create-test-db": "db-migrate --config database_create.json db:create store_test && db-migrate up --env=test",
    "up": "db-migrate up",
    "down": "db-migrate down *",
    "lint": "eslint ./src/**/*.ts",
    "prettier": "prettier --config .prettierrc ./src/**/*.ts --write",
    "build": " npx tsc",
    "jasmine": "jasmine",
    "test": " set NODE_ENV_TEST=test && npx tsc  && db-migrate --env test up  && jasmine && db-migrate --env test down * ",
    "watch-ts": "tsc -w",
    "watch-build": "nodemon dist/server.js",
    "dev": "concurrently \"npm run watch-ts\" \"npm run watch-build\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.3.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "pg": "^8.11.3",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@tsconfig/node-lts-strictest-esm": "^18.12.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/eslint": "^8.44.2",
    "@types/express": "^4.17.17",
    "@types/jasmine": "^4.3.5",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.5.6",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "concurrently": "^8.2.1",
    "eslint": "^8.48.0",
    "jasmine": "^5.1.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "node": "^20.5.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "supertest": "^6.3.3",
    "tsc-watch": "^6.0.4",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  }
}
